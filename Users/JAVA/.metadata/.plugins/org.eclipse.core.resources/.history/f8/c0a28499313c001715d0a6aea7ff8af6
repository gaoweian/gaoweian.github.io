package com.fanshe;

public class Main {

	public static void main(String[] args){
//		Main m = new Main();
//		System.out.println(m.getClass().getName());
		
		Class<?> c1 = null;
		Class<?> c2 = null;
		Class<?> c3 = null;
		try {
			c1 = Class.forName("com.fanshe.Main");
			c2 = new Main().getClass();
			c3 = Main.class;
			System.out.println("类名称"+c1);
			System.out.println("类名称"+c2);
			System.out.println("类名称"+c3);
		} catch (ClassNotFoundException e) {
			e.printStackTrace();
		}
	}
}
